{
  "name": "Ha11y",
  "action": {},
  "manifest_version": 3,
  "version": "0.1",
  "description": "Displays the A11y score of sites when searching in google.",
  "permissions": [
    "webRequest",
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions":[
    "*://*.google.com/search?q=*"
  ],
  "icons": { "16": "icon16.png",
    "48": "icon48.png",
   "128": "icon128.png" },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "index.html"

}
